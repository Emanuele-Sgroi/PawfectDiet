{
  "expo": {
    "name": "PawfectDiet",
    "slug": "PawfectDiet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.emanuele.pawfectdiet"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": ["expo-font", "expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "7681adb9-3520-47a1-a9bb-b7451d9f62f2"
      }
    },
    "scheme": "com.emanuele.pawfectdiet"
  }
}
